{
    "root": false,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "rules": {
        // 0 - off
        // 1 - warn
        // 2 - error 
        "no-console": 2,
        "no-duplicate-imports": 2,
        "no-self-compare": 2,
        "default-case": 2,
        "default-case-last": 2,
        "camelcase": 1,
        "prettier/prettier": 2,
        "no-unused-vars": 1
    }
}